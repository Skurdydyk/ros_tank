        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 -0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </visual>
    </link>

    <!-- Joint - chassis / right middle wheel -->

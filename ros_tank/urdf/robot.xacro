            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 -0.015 0" />
            <geometry>

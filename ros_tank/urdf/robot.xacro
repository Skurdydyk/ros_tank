<?xml version="1.0" ?>
<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find ros_tank)/urdf/gazebo.xacro" />

    <!-- Laser scan -->
    <joint name="joint_laser_scan_chassis" type="fixed">
        <origin rpy="0 0 0" xyz="-0.01 0 0.11" />
        <child link="link_laser_scan" />
        <parent link="link_chassis" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <link name="link_laser_scan">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://ros_tank/meshes/lidarA1.stl" pose="0 0 0" scale="0.00111 0.00111 0.0011"/>
                <!-- <cylinder radius="0.04" length="0.04"/> -->
            </geometry>
            <material name="Red">
                <color rgba="0.7 0.1 0.1 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.04" length="0.04"/>
            </geometry>
        </collision>
    </link>

    <!-- Camera -->
    <joint name="camera_joint" type="fixed">
        <parent link="link_chassis"/>
        <child link="camera_link"/>
        <origin xyz="0.11 0 0.04" rpy="0 0 0"/>
    </joint>

    <link name="camera_link">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.02 0.08 0.038"/>
            </geometry>
        </collision>
        <visual>
            <geometry>
                <mesh filename="package://ros_tank/meshes/camera.stl" pose="0 0 0" scale="0.00111 0.00111 0.0011"/>
                <!-- <box size="0.02 0.08 0.038"/> -->
            </geometry>
        </visual>
    </link>

    <!-- Link - chassis -->
    <link name="link_chassis">
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <box size="0.22 0.14 0.25" />
            </geometry>
        </collision>
        <visual>
            <geometry>
                <!-- <box size="0.22 0.14 0.25" /> -->
                <mesh filename="package://ros_tank/meshes/tank_body.stl" pose="0 0 0" scale="0.00111 0.00111 0.0011"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / left front wheel -->
    <joint name="joint_chassis_left_front_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.065 0.077 -0.12" />
        <child link="link_front_left_wheel" />
        <parent link="link_chassis" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left front wheel -->
    <link name="link_front_left_wheel">
        <inertial>
            <mass value="10" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </visual>
    </link>

    <!-- Joint - chassis / left middle wheel -->
    <joint name="joint_chassis_left_middle_wheel" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.077 -0.12" />
        <child link="link_middle_left_wheel" />
        <parent link="link_chassis" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>


<?xml version="1.0" ?>

<robot name="robot">
    <!-- Link - chassis -->
    <link name="link_chassis">
        <inertial>
            <mass value="10" />
            <origin xyz="0 0 0.3" rpy="0 0 0" />
            <inertia ixx="1.5417" ixy="0" ixz="0" iyy="3.467" iyz="0" izz="4.742" />
        </inertial>

        <collision>
            <geometry>
                <box size="2 1.3 0.4" />
            </geometry>
        </collision>

        <visual>
            <geometry>
                <!-- <box size="0.22 0.14 0.25" /> -->
                <mesh filename="package://ros_tank/meshes/tank_robot.stl" pose="0 0 0" scale="0.00111 0.00111 0.0011"/>
            </geometry>
            <material name="Red">
                <color rgba="1 0 0 1" />
            </material>
        </visual>
    </link>

    <!-- Joint - chassis / left front wheel -->
    <joint name="joint_chassis_left_front_wheel" type="fixed">
        <origin rpy="0 0 0" xyz="0.065 0.077 -0.12" />
        <child link="link_front_left_wheel" />
        <parent link="link_chassis" />
        <axis rpy="0 0 0" xyz="0 1 0" />
        <limit effort="10000" velocity="1000" />
        <joint_properties damping="1.0" friction="1.0" />
    </joint>

    <!-- Link - left front wheel -->
    <link name="link_front_left_wheel">
        <collision>
            <origin rpy="1.5707 0 0" xyz="0 -0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </collision>
        <visual>
            <origin rpy="1.5707 0 0" xyz="0 0.015 0" />
            <geometry>
                <cylinder length="0.03" radius="0.02"/>
            </geometry>
        </visual>
    </link>

    <!-- Joint - chassis / left middle wheel -->
